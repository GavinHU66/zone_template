ner\">\n        <div class=\"pi-left\">\n\n            <div class=\"pi-left-title\">个人中心</div>  \n            <div class=\"account-setting\">\n                <div class=\"subtitle\">账号设置</div>\n                <ul class=\"pi-list\">\n                  <li>个人资料</li>\n                  <li>修改头像</li>\n                  <li>修改密码</li>\n                  <li>账号安全</li>\n                  <li>任教信息</li>\n                  <li>课程管理</li>\n                  <li>访问权限</li>\n                  <li>装扮记录</li>\n                </ul>\n            </div>\n\n            <div class=\"my-coin\">\n                <div class=\"subtitle\">积分云币</div>\n                <ul class=\"pi-list\">\n                  <li>我的积分</li>\n                  <li>我的账单</li>\n                </ul>\n            </div>\n\n            <div class=\"other-setting\">\n                <div class=\"subtitle\">其他设置</div>\n                <ul class=\"pi-list\">\n                  <li>我的应用库</li>\n                </ul>\n            </div>\n        </div>\n\n        <div class=\"pi-right\">\n\n            <div class=\"pi-right-title\">个人资料</div>\n            <div class=\"personal-info\">\n              <ul class=\"pi-list\">\n                <li>账号：{{accountId}}</li>\n                <li>姓名：{{username}}</li>\n                <li>手机：{{phone}}</li>\n                <li>邮箱：{{emailAddress}}</li>\n                <li>性别：{{gender}}</li>\n\n                <li>主页网址：{{personalWeb}}</li>\n                <li>个性签名：{{feeling}}</li>\n                <li>个人简介：{{description}}</li>\n              </ul>\n            </div>\n            <div class=\"pi-edit-btn\">\n              <button @click=\"handleEditPI\">编辑</button>\n            </div>\n        </div>\n    </div>\n\n  </div>\n</template>\n<script>\nimport ZoneBar from \"@/components/navBars/ZoneBar\";\n\nexport default {\n  name: \"PersonalInfo\",\n  \n  data () {\n    return {\n      accountId: \"jz_js006\",\n      username:\"姜凯\",\n      phone: \"未绑定手机账号\",\n      emailAddress: \"未绑定邮