' && url !== '') {\n        var payload = {\n          linkName: name,\n          shouldShow: true,\n          linkUrl: url\n        };\n        this.$store.dispatch('navBarLinks/addLink', payload);\n        alert(\"已添加\");\n        this.shouldShowAddLink = false;\n      } else {\n        alert(\"请将链接名称或链接地址填写完整。\");\n      }\n    },\n    cancelAdding: function cancelAdding() {\n      this.shouldShowAddLink = false;\n    },\n    saveEditing: function saveEditing() {\n      var payload = {\n        oldLinkName: this.oldLinkName,\n        newLinkName: this.editNamePlaceholder,\n        newLinkUrl: this.editUrlPlaceholder\n      };\n      this.$store.dispatch('navBarLinks/editLink', payload);\n      alert(\"已保存\");\n      this.shouldShowEditLink = false;\n    },\n    cancelEditing: function cancelEditing() {\n      // console.log(\"in methods: cancelEditing\");\n      this.shouldShowEditLink = false;\n    },\n    deleteLink: function deleteLink() {\n      var payload = {\n        linkName: this.editNamePlaceholder\n      };\n      this.$store.dispatch('navBarLinks/deleteLink', payload);\n      alert(\"已删除\");\n      this.shouldShowEditLink = false;\n    }\n  }\n};",{"version":3,"sources":["LinkCenter.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAuCA,eAAA;AACA,EAAA,IAAA,EAAA,YADA;AAGA,EAAA,IAHA,kBAGA;AACA,WAAA;AACA,MAAA,iBAAA,EAAA,KADA;AAEA,MAAA,kBAAA,EAAA,KAFA;AAGA,MAAA,mBAAA,EAAA,EAHA;AAIA,MAAA,kBAAA,EAAA,EAJA;AAKA,MAAA,WAAA,EAAA;AALA,KAAA;AAOA,GAXA;AAaA,EAAA,OAAA,EAAA;AACA,IAAA,YADA,wBACA,IADA,EACA;AACA,WAAA,kBAAA,GAAA,IAAA;AACA,WAAA,iBAAA,GAAA,KAAA;AACA,WAAA,mBAAA,GAAA,IAAA,CAAA,QAAA;AACA,WAAA,kBAAA,GAAA,IAAA,CAAA,OAAA;AACA,WAAA,WAAA,GAAA,IAAA,CAAA,QAAA;AACA,KAPA;AAQA,IAAA,OARA,qBAQA;AACA,WAAA,kBAAA,GAAA,KAAA;AACA,WAAA,iBAAA,GAAA,IAAA;AACA,KAXA;AAaA,IAAA,UAbA,wBAaA;AACA,UAAA,IAAA,GAAA,QAAA,CAAA,cAAA,CAAA,UAAA,EAAA,KAAA;AACA,UAAA,GAAA,GAAA,QAAA,CAAA,cAAA,CAAA,SAAA,EAAA,KAAA;;AACA,UAAA,IAAA,KAAA,EAAA,IAAA,GAAA,KAAA,EAAA,EAAA;AACA,YAAA,OAAA,GAAA;