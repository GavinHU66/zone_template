t.id === 4\"/>\n            <UserInfo v-if=\"showUserInfo\"/> -->\n            <div class=\"zone-module\"><ResizableModule moduleName=\"userInfo\" demotxt=\"个人资料\" :draggable=\"true\" :resizable=\"true\" v-if=\"element.id === 1 && $store.state.item.shouldShowUserInfo\"/></div>\n            <div class=\"zone-module\"><ResizableModule moduleName=\"MsgBoard\" demotxt=\"留言板\" :draggable=\"true\" :resizable=\"true\" v-if=\"element.id === 5 && $store.state.item.shouldShowMsgBoard\"/></div>\n            <div class=\"zone-module\"><ResizableModule moduleName=\"personalZone\" demotxt=\"个人空间\" :draggable=\"true\" :resizable=\"true\" v-if=\"element.id === 8 && $store.state.item.shouldShowPersonalZone\"/></div>\n            <div class=\"zone-module\"><ResizableModule moduleName=\"recentVisitors\" demotxt=\"最近访客\" :draggable=\"true\" :resizable=\"true\" v-if=\"element.id === 2 && $store.state.item.shouldShowRecentVisitors\"/></div>\n\n          </li>\n        </transition-group>\n      </draggable>\n    </div>\n  </div>\n</template>\n<script>\nimport draggable from \"vuedraggable\";\nimport MyEssay from \"@/components/panelApps/MyEssay\";\nimport CurrentVisitor from \"@/components/panelApps/CurrentVisitor\";\nimport UserInfo from \"@/components/panelApps/UserInfo\";\nimport LatestNews from \"@/components/panelApps/LatestNews\";\nimport MsgBoard from \"@/components/panelApps/MsgBoard\";\nimport ResizableModule from \"@/components/ResizableModule\";\n\nexport default {\n  name: \"TemplateCustomization\",\n  components: {\n    draggable,\n    MyEssay,\n    CurrentVisitor,\n    UserInfo,\n    LatestNews,\n    MsgBoard,\n    ResizableModule\n  },\n  data() {\n    return {\n      display: \"Transition\",\n      lists:[\n        [\n          { name: \"John\", id: 1 },\n          { name: \"Joao\", id: 2 },\n          { name: \"Jean\", id: 3 },\n          { name: \"Gerard\", id: 4 },\n        ],\n        [\n          { name: \"Juan\", id: 5 },\n          { name: \"Edgard\", id: 6 },\n   